{
  "name": "todo-hi",
  "version": "2.0.0",
  "main": "app.js",
  "private": true,
  "scripts": {
    "clean": "rimraf node_modules/",
    "build": "webpack --config webpack.config.js",
    "build-dev": "webpack --config webpack.dev.config.js",
    "deploy:dynamic": "git push heroku master",
    "dev": "webpack --config webpack.dev.config.js && npm run start",
    "deploy": "npm-run-all deploy:*",
    "lint:js": "eslint --fix .",
    "lint:css": "stylelint **/*.css",
    "lint": "npm-run-all lint:*",
    "start": "NODE_ENV=development node app.js",
    "test": "mocha --recursive tests",
    "precommit": "npm-run-all lint:* && npm test",
    "prepush": "npm-run-all lint:* && npm test",
    "production": "webpack --config webpack.config.js && node app.js"
  },
  "license": "MIT",
  "dependencies": {
    "autoprefixer": "6.3.3",
    "body-parser": "1.15.0",
    "css-loader": "0.23.1",
    "cssnano": "3.5.2",
    "express": "4.13.4",
    "extract-text-webpack-plugin": "1.0.1",
    "hbs": "4.0.0",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-redux": "^4.4.1",
    "redux": "^3.3.1",
    "style-loader": "0.13.0",
    "webpack": "1.12.14"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "3.5.0",
    "css-loader": "0.23.1",
    "eslint": "2.3.0",
    "eslint-config-xo": "0.12.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "1.0.1",
    "husky": "0.11.3",
    "mocha": "2.4.5",
    "morgan": "1.7.0",
    "npm-run-all": "1.5.2",
    "postcss-loader": "0.8.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.0",
    "stylelint": "5.0.0",
    "stylelint-config-standard": "4.0.0",
    "supertest": "1.2.0",
    "webpack": "1.12.14"
  },
  "engines": {
    "node": "4.4.0"
  }
}
