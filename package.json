{
  "name": "notes",
  "version": "1.0.0",
  "main": "app.js",
  "scripts": {
    "clean": "rimraf node_modules/",
    "build": "webpack",
    "deploy:dynamic": "git push heroku master",
    "deploy:static": "surge -p ./public -d urfu2015-notes.surge.sh",
    "dev": "webpack --config webpack.dev.config.js && npm start",
