    "morgan": "1.7.0"
  },
  "devDependencies": {
    "autoprefixer": "6.3.3",
    "css-loader": "0.23.1",
    "cssnano": "3.5.2",
    "chai": "3.5.0",
    "eslint": "2.3.0",
    "eslint-config-xo": "0.12.0",
    "extract-text-webpack-plugin": "1.0.1",
    "husky": "0.11.3",
    "mocha": "2.4.5",
    "npm-run-all": "1.5.2",
    "postcss-loader": "0.8.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.0",
    "stylelint": "5.0.0",
    "stylelint-config-standard": "4.0.0",
